{
  "name": "goferai/util",
  "type": "library",
  "description": "Library of PHP utility functions",
  "keywords": [
    "gofer",
    "utility functions"],
  "homepage": "https://github.com/goferai/gofer-util",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Chip Allen",
      "email": "chip@gofer.co",
      "role": "Developer"
    }
  ],
  "autoload": {
